#include "helium3.h"

int main(int argc, char **argv)
{
    he3SystemInfo sysinfo;

    if (he3_startup()) {
        /* Last point to apeal error. Return as fatal */
        he3_err(status);
    }

    if (he3_run_app(argc, argv);) {
        he3_err(status);
    }

    if (he3_shutdown();) {
        he3_err(status);
    }

    return 0;
}
